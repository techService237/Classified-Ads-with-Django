<script>
function coinAfriqueSearchFunctinality() {
	/* event.preventDefault(); */
	var keyword = document.getElementById("pi-keyword").value;
	var category = document.getElementById("pi-category").value;
	var country = document.getElementById("pi-country").value;
	var query = 'https://' + country + '.coinafrique.com/search?category=' + category + '&keyword=' + keyword;
	if ( ! (document.getElementById("pi-country").value == 0) ) {
	window.open(query, "_blank");
	}
}
</script>

